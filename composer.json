        "drupal/cacheexclude": "^2.0",
        "drupal/redis": "^1.7",
        "drupal/config_filter": "^2.6",
        "drupal/quickedit": "^1.0",
        "drupal/rdf": "^2.1",
        "drupal/warmer": "^2.0",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/smart_date": "^4.1@beta",
